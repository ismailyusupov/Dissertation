cd "L:\THEATRES_STUDENTS\_Ismail"
use fxdata
cd "L:\THEATRES_STUDENTS\_Ismail\2004"
destring date2
keep if date2 = 2004
forvalues i = 1/240 {
    statsby _b _se, by(datetime) saving(results`i'): reg dlp x_`i'
}
forvalues i=2/240 {
	use results`i' , clear 
	merge 1:1 datetime using results1
	save results1,replace
	use results1
	drop _merge
	save results1,replace
}


